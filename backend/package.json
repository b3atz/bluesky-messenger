{
  "name": "bluesky-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:node": "node --import tsx/esm src/index.ts",
    "dev:legacy": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "heroku-postbuild": "npm run build",
    "migration:create": "tsx ./node_modules/.bin/mikro-orm migration:create",
    "migration:up": "tsx ./node_modules/.bin/mikro-orm migration:up",
    "migration:down": "tsx ./node_modules/.bin/mikro-orm migration:down"
  },
  "dependencies": {
    "@atproto/api": "^0.15.10",
    "@atproto/jwk-jose": "^0.1.6",
    "@atproto/oauth-client": "^0.3.19",
    "@atproto/oauth-client-node": "^0.2.20",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/jwt": "^9.1.0",
    "@mikro-orm/core": "^6.4.15",
    "@mikro-orm/migrations": "^6.4.15",
    "@mikro-orm/seeder": "^6.4.15",
    "@mikro-orm/sqlite": "^6.4.15",
    "dotenv": "^16.4.1",
    "fastify": "^5.3.3",
    "sqlite3": "^5.1.7",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.15",
    "@swc/core": "^1.11.24",
    "@types/node": "^22.15.17",
    "pino-pretty": "^13.0.0",
    "ts-node": "^10.9.2",
    "tsconfig": "^7.0.0",
    "tsx": "^4.7.0"
  }
}